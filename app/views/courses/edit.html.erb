<h2>Edit Course Information</h2>

<%= form_for [@semester, @course] do |f| %>
<div class="field">
  <%= f.label :name %>
  <%= f.text_field :name %>
</div>

<div class="field">
  <%= f.label :code %>
  <%= f.text_field :code %>
</div>


<div class="field">
  <%= f.submit 'Update'%>
</div>
<% end %>
